# System.

if [ $show -eq 1 ]; then
  writeln;
### Get Some Values BeforeHand.
  local -a system_epoc=(`/bin/date "+%s"`);
  local system_date="`/bin/date`";
### Display System Information.
  writeln "\040NETworkMONitor (v$version) Date Reporter.";
  writeln "\040------------------------------------";
  writeln "\040date            $system_date";
  writeln "\040epoc            $system_epoc";
  writeln "\040-------------------------- ---- -- -";
  return 0;
fi

return 1;
